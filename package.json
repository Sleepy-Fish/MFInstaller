{
  "name": "mfinstaller",
  "version": "1.0.0",
  "description": "Installer for MonsterFarm",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "rm -rf dist && npm run build-mac && npm run build-lin && npm run build-win --asar",
    "build-mac": "electron-packager . MFInstaller --platform=darwin --out=dist",
    "build-lin": "echo \"No Linux Build\"",
    "build-win": "echo \"No Windows Build\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "J Riley Harrington <jrileyharrington@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-packager": "^13.0.0"
  },
  "dependencies": {
    "mustard-ui": "0.0.8"
  }
}
